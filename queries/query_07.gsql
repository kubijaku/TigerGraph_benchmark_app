BEGIN
CREATE OR REPLACE DISTRIBUTED QUERY query_07(VERTEX<Entity> inputNode) FOR GRAPH ADS { 
  Start = {inputNode};
  
  Successors = SELECT t FROM Start:s -(Relation:e)-> Entity:t;
  GrandChildren = SELECT t FROM Successors:s -(Relation:e)-> Entity:t;
  
  PRINT GrandChildren; 
}
USE GRAPH ADS
INSTALL QUERY query_07
END
