BEGIN
CREATE OR REPLACE DISTRIBUTED QUERY query_08(VERTEX<Entity> inputNode) FOR GRAPH ADS { 
  Start = {inputNode};

  Parents = SELECT t FROM Start:s -(Relation)-> Entity:t;
  Grandparents = SELECT t FROM Parents:p -(Relation)-> Entity:t;

  PRINT Grandparents;
}
USE GRAPH ADS
INSTALL QUERY query_08
END