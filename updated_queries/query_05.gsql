CREATE OR REPLACE DISTRIBUTED QUERY query_05(VERTEX<Concept> inputNode) FOR GRAPH ConceptGraph { 
  Start = {inputNode};
  
  Successors = SELECT t FROM Start:s -(RELATION:e)-> Concept:t;
  Predecessors = SELECT t FROM Start:s -(RELATION_REV:e)-> Concept:t;
  
  Neighbors = Successors UNION Predecessors;
  
  PRINT Neighbors;
}