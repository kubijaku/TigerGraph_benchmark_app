BEGIN
CREATE OR REPLACE DISTRIBUTED QUERY query_02(STRING startVid) FOR GRAPH ADS {

    SumAccum<INT> @@succ_count;

    tmp = SELECT next
    FROM Entity:current - (Relation:e) -> Entity:next
    WHERE current.id = startVid
    ACCUM @@succ_count += 1;

    PRINT @@succ_count AS successor_count;
}
INSTALL QUERY query_02
END